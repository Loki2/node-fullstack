<%- include('../../partial/adminheader') %>
<%- include('../../partial/adminnavbar') %>
<%- include('../../partial/adminsidebar') %>
  <main>
    <div class="container-fluid">
      <section class="section card mb-5">

        <div class="card-body">
          <h5 class="pb-5">Create New Category Blogs</h5>
          <form action="" method="post">
          <div class="row">
            <div class="col-md-6 mb-4">
              <div class="md-form">
                <input type="text" id="title" name="title" class="form-control" required>
                <label for="" class="">ຫົວຂໍ້:</label>
              </div>
            </div>

            <div class="col-md-12 mb-4">
              <div class="md-form">
                <input type="text" id="desc" name="desc" class="form-control" required>
                <label for="" class="">ອະທິບາຍຫົວຂໍ້:</label>
              </div>
            </div>
            <div class="col-md-6 mb-4"> 
            <form action="#">
              <div class="md-form">
                <div class="file-field">
                  <div class="btn btn-primary btn-sm float-left">
                    <span>Choose files</span>
                    <input type="file" multiple>
                  </div>
                  <div class="file-path-wrapper">
                    <input class="file-path validate" type="text" placeholder="Upload one or more files">
                  </div>
                </div>
              </div>
            </form>
            </div>         
        </div>
        <div class="form-group">
          <a href="/admin/blogs" class="btn btn-teal pull-right">Submit</a>
        </div>
      </form>
      </section>
    </div>
  </main>
  <!-- Main layout -->
<%- include('../../partial/adminscript') %>
